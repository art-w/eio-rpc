(library
 (name keyval_rpc_proto)
 (public_name eio-rpc.keyval-rpc-proto)
 (flags :standard -w +a-4-40-41-42-44-45-48-66 -warn-error +a)
 (libraries pbrt pbrt_services)
 (instrumentation
  (backend bisect_ppx)))

(rule
 (targets keyval.ml keyval.mli)
 (deps
  (glob_files *.proto))
 (action
  (run ocaml-protoc keyval.proto --binary --services --ml_out .)))

(rule
 (alias runtest)
 (action
  (diff keyval.ml.expected keyval.ml)))

(rule
 (alias runtest)
 (action
  (diff keyval.mli.expected keyval.mli)))
